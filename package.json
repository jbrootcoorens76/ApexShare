{
  "name": "apexshare",
  "version": "1.0.0",
  "description": "Serverless motorcycle training video sharing system built on AWS",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "cdk:deploy": "cdk deploy --all",
    "cdk:destroy": "cdk destroy --all",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "sam:build": "sam build",
    "sam:deploy": "sam deploy",
    "sam:local": "sam local start-api",
    "deploy:dev": "npm run cdk:deploy -- --profile dev",
    "deploy:staging": "npm run cdk:deploy -- --profile staging",
    "deploy:prod": "npm run cdk:deploy -- --profile prod"
  },
  "keywords": [
    "aws",
    "serverless",
    "lambda",
    "s3",
    "dynamodb",
    "ses",
    "cloudfront",
    "api-gateway",
    "motorcycle",
    "training",
    "video-sharing"
  ],
  "author": "ApexShare Team",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "aws-cdk": "^2.95.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.204.0",
    "@aws-cdk/aws-cloudfront": "^1.204.0",
    "@aws-cdk/aws-dynamodb": "^1.204.0",
    "@aws-cdk/aws-lambda": "^1.204.0",
    "@aws-cdk/aws-s3": "^1.204.0",
    "@aws-cdk/aws-ses": "^1.204.0",
    "@aws-cdk/core": "^1.204.0",
    "@aws-sdk/client-cloudwatch": "^3.411.0",
    "@aws-sdk/client-cost-explorer": "^3.411.0",
    "@aws-sdk/client-dynamodb": "^3.411.0",
    "@aws-sdk/client-s3": "^3.411.0",
    "@aws-sdk/client-ses": "^3.411.0",
    "@aws-sdk/client-sns": "^3.411.0",
    "aws-lambda": "^1.0.7"
  }
}