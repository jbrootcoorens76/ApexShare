  "keywords": [
    "aws",
    "lambda",
    "authentication",
    "jwt",
    "apexshare"
  ],
  "author": "ApexShare Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
