{
  "name": "apexshare-frontend",
  "version": "1.0.0",
  "description": "React frontend for ApexShare motorcycle training video sharing system",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "test:performance": "vitest run src/tests/uploadQueueManager.performance.test.ts",
    "test:functional": "vitest run src/tests/uploadQueueManager.functional.test.ts",
    "test:load": "vitest run src/tests/uploadQueueManager.load.test.ts",
    "test:integration": "vitest run src/tests/uploadQueueManager.integration.test.ts",
    "test:edge-cases": "vitest run src/tests/uploadQueueManager.edge-cases.test.ts",
    "test:benchmark": "vitest run src/tests/uploadQueueManager.benchmark.test.ts",
    "test:production": "vitest run src/tests/production-readiness.test.ts",
    "test:comprehensive": "node run-comprehensive-tests.js",
    "test:quick": "vitest run src/tests/uploadQueueManager.test.ts",
    "validate": "npm run typecheck && npm run lint && npm run test:quick"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "axios": "^1.6.2",
    "react-dropzone": "^14.2.3",
    "react-query": "^3.39.3",
    "zustand": "^4.4.7",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.1.0",
    "lucide-react": "^0.294.0",
    "react-hot-toast": "^2.4.1",
    "framer-motion": "^10.16.5",
    "uuid": "^9.0.1",
    "@tailwindcss/forms": "^0.5.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "jsdom": "^23.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}